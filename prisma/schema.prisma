datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  createdAt DateTime @default(now())
}

model MccLink {
  id                 String   @id @default(cuid())
  userId             String
  mccCid             String   @default("")
  refreshCipherBytes Bytes
  scopes             String[]
  createdAt          DateTime @default(now())
}
